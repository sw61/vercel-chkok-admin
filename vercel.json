{
  "rewrites": [
    {
      "source": "/grafana/:path*",
      "destination": "http://jenkins.chkok.kr:3000/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/grafana/(.*)",
      "headers": [
        { "key": "X-Forwarded-Proto", "value": "https" },

        { "key": "Host", "value": "jenkins.chkok.kr:3000" }
      ]
    }
  ]
}
